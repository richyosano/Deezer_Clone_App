{
  "parser": "@babel/eslint-parser",
	"plugins": ["@babel"],
  "rules": {
    "@babel/new-cap": 1,
    "@babel/no-invalid-this": 2,
    "@babel/no-unused-expressions": 1,
    "@babel/object-curly-spacing": [
      1,
      "always",
      {
        "objectsInObjects": false,
        "arraysInObjects": false
      }
    ],
    "@babel/semi": 1
  }
}
